<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="controller.LoginController"
          prefHeight="400.0" prefWidth="600.0">
    <Label GridPane.rowIndex="0" text="User" />
    <TextField GridPane.rowIndex="1" fx:id="userField" />

    <Label GridPane.rowIndex="2" text="Password" />
    <TextField GridPane.rowIndex="3" fx:id="passwordField" />

    <Button GridPane.rowIndex="4" text="Submit" onAction="#submit" />

</GridPane>
